<%- model_class = BaseObjective -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:parent_type) %>:</strong></dt>
  <dd><%= @base_objective.parent_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:parent_id) %>:</strong></dt>
  <dd><%= @base_objective.parent_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:age) %>:</strong></dt>
  <dd><%= @base_objective.age %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sex) %>:</strong></dt>
  <dd><%= @base_objective.sex %></dd>
  <dt><strong><%= model_class.human_attribute_name(:culture_date) %>:</strong></dt>
  <dd><%= @base_objective.culture_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:serviceb) %>:</strong></dt>
  <dd><%= @base_objective.serviceb %></dd>
  <dt><strong><%= model_class.human_attribute_name(:accession_number) %>:</strong></dt>
  <dd><%= @base_objective.accession_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:icu) %>:</strong></dt>
  <dd><%= @base_objective.icu %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nosocomial) %>:</strong></dt>
  <dd><%= @base_objective.nosocomial %></dd>
  <dt><strong><%= model_class.human_attribute_name(:code_number) %>:</strong></dt>
  <dd><%= @base_objective.code_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:shipment_id) %>:</strong></dt>
  <dd><%= @base_objective.shipment_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              base_objectives_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_base_objective_path(@base_objective), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              base_objective_path(@base_objective),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
