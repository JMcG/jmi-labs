<%- model_class = Shipment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Objective:</strong></dt>
  <dd><%= @shipment.objective_name %></dd>
  <dt><strong>Target Isolates</strong></dt>
  <dd><%= @shipment.target_isolates %></dd>
</dl>

<% @shipment.isolates.each do |isolate| %>
  <%= render "isolate", :isolate => isolate, :model_class => model_class %>
<% end -%>

<%# @shipment.isolates.each do |isolate| %>
  
  
  <%#= render isolate %>

<% #end -%>



<%= link_to t('.back', :default => t("helpers.links.back")),
              shipments_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_shipment_path(@shipment), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              shipment_path(@shipment),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
