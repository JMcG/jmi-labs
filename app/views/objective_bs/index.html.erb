<%- model_class = ObjectiveB -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:jmi_number) %></th>
      <th><%= model_class.human_attribute_name(:specimen_typea) %></th>
      <th><%= model_class.human_attribute_name(:bacteria_type) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @objective_bs.each do |objective_b| %>
      <tr>
        <td><%= link_to objective_b.id, objective_b_path(objective_b) %></td>
        <td><%= objective_b.jmi_number %></td>
        <td><%= objective_b.specimen_typea %></td>
        <td><%= objective_b.bacteria_type %></td>
        <td><%=l objective_b.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_objective_b_path(objective_b), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      objective_b_path(objective_b),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_objective_b_path,
            :class => 'btn btn-primary' %>
